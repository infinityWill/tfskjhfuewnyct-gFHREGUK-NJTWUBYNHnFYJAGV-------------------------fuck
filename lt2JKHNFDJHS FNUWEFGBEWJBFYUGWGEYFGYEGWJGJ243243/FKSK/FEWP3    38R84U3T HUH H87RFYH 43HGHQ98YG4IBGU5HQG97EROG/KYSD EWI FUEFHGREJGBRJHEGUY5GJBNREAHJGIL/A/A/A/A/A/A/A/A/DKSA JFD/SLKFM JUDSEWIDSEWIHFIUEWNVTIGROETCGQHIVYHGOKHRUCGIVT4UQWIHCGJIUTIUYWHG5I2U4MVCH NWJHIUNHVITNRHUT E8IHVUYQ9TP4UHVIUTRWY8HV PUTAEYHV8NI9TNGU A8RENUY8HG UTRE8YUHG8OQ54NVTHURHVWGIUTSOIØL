                        if v.Text ~=  "" then
                            if v.Text ~=  "l" then
                                if v.Text ~= "Ancestor" then
                   v.TextColor3 = Color3.fromRGB(r,g,b)
                    end
                  end
                end
               end
            end
          end
     end,
    Color3.fromRGB(255,255,255)
